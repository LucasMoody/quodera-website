{
  "name": "quodera-website",
  "version": "0.8.0",
  "description": "Quodera website",
  "main": " ",
  "scripts": {
    "build:dev": "URL=http://quodera.com  gulp build",
    "build:prod": "NODE_ENV=production URL=http://quodera.com gulp build",
    "clean": "rm -rf dist/",
    "install:aws_cli": "pip install awscli",
    "deploy:dev": "npm run clean && npm run build:dev && AWS_PROFILE=website aws s3 sync dist/ s3://quodera.com-dev && opn http://quodera.com-dev.s3-website.eu-central-1.amazonaws.com/",
    "deploy:prod": "npm run clean && npm run build:prod && AWS_PROFILE=website aws s3 sync dist/ s3://quodera.com && opn http://quodera.com.s3-website.eu-central-1.amazonaws.com/",
    "start": "URL=http://quodera.com && opn $(pwd)/dist/index.html && gulp watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/LucasMoody/quodera-website.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/LucasMoody/quodera-website/issues"
  },
  "homepage": "https://github.com/LucasMoody/quodera-website#readme",
  "dependencies": {
    "aws-sdk": "^2.229.1",
    "gulp": "^3.9.1",
    "gulp-autoprefixer": "^4.1.0",
    "gulp-clean-css": "^3.9.3",
    "gulp-compile-handlebars": "^0.6.1",
    "gulp-concat": "^2.6.1",
    "gulp-if": "^2.0.2",
    "gulp-less": "^3.4.0",
    "gulp-rename": "^1.2.2",
    "gulp-sitemap": "^5.0.0",
    "gulp-uglify": "^3.0.0",
    "gulp-watch": "^5.0.0",
    "opn": "^5.3.0",
    "opn-cli": "^3.1.0",
    "pump": "^2.0.1",
    "run-sequence": "^2.2.1"
  }
}
